{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "RedactorServiceOptions": {
    "PropertiesToRedact": [
      "Password",
      "Salt",
      "PasswordHash",
      "ActivateEmailToken",
      "EmailPreferenceToken"
    ],
    "RegexValuesToRedact": []
  },
  "AccountProxyOptions": {
    "BaseURL": "https://localhost:6001/",
    "HttpClientTimeoutInSeconds": 300,
    "CreateAccount": {
      "TimeoutInSeconds": 30,
      "Retrys": 2,
      "Resource": "api/v1/Account/Create"
    }
  }
}
